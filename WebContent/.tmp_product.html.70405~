<script type="text/javascript">
	$(function(){
		// 获取产品分类
		$.ajax({
			url : "getProductSort",
			dataType : "json",
			success : function(data){
				data.forEach(function(item){
					$("<li><a href='product.html?sortID=" + item.ID + "'>" + item.sortName + "</a></li>").appendTo($(".menuList"));
				});
			}
		});
		
		// 根据url中分类ID获取相关产品
		var sortID = getPara("sortID");
		$.ajax({
			url : "getProduct",
			data : "sortID=" + sortID,
			dataType : "json",
			success : function(data){
				data.forEach(function(item){
					$("<div class='product'><div class='productImg'><a href='productDetail.html?productID=" + item.ID + "'><img src='" + item.imgSrc + "' /></a></div><div class='productName'>" + item.name + "</div></div>").appendTo($(".rightContainer"));
				});
			}
		});
	});
</script>
