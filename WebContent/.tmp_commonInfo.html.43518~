<script type="text/javascript">
	var infos;
	$(function(){
		// 获取公共信息
		$.ajax({
			url : "getCommonInfo",
			dataType : "json",
			success : function(data){
				infos = data;
				data.forEach(function(item){
					$("<li><a href='commonInfo.html?name=" + item.name + "'>" + item.name + "</a></li>").appendTo($(".menuList"));
				});

				// 是否显示某个指定品牌介绍
				var name = getPara("name");
				if(name){
					infos.forEach(function(item){
						if(item.name == name){
							$("<div class='commonInfo'>" + item.content + "</div>").appendTo($(".rightContainer"));
						}
					});
				}
				else{
					// 显示第一个
					$("<div class='commonInfo'>" + infos[0].content + "</div>").appendTo($(".rightContainer"));
				}
			}
		});
	});
</script>
